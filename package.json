{
  "name": "react-demo-drag-selector",
  "version": "0.1.0",
  "description": "A demo React.js application for Drag Selector Libs (@mornya/drag-selector-libs).",
  "keywords": [
    "mornya",
    "react",
    "vessel",
    "demo",
    "drag-selector"
  ],
  "homepage": "https://mornya.github.io/react-demo-drag-selector",
  "license": "MIT",
  "author": "mornya <mornya78@gmail.com>",
  "private": true,
  "repository": "https://github.com/mornya/react-demo-drag-selector",
  "bugs": "https://github.com/mornya/react-demo-drag-selector/issues",
  "types": "./types/index.d.ts",
  "scripts": {
    "login": "react-vessel login",
    "lint": "react-vessel lint all",
    "clean": "react-vessel clean",
    "build": "react-vessel build",
    "start": "react-vessel start",
    "test": "react-vessel test",
    "analyze": "react-vessel analyze",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "dependencies": {
    "@mornya/drag-selector-libs": "^1.3.0",
    "@mornya/react-helper": "^0.6.1",
    "immer": "latest",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/jest-diff": "latest",
    "@types/node": "^12.0.3",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^4.3.3",
    "gh-pages": "^2.0.1",
    "jest": "^24.8.0",
    "jest-transform-stub": "latest",
    "prop-types": "^15.7.2",
    "ts-jest": "latest",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": {
    "/mock/": {
      "target": "http://localhost:8080",
      "pathRewrite": {
        "^/mock/": ""
      },
      "secure": false,
      "changeOrigin": false,
      "ws": false,
      "xfwd": true
    }
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": true
    }
  },
  "jest": {
    "transform": {
      ".[t|j]sx?$": "ts-jest",
      ".(bmp|gif|jpe?g|png|svg|webp|s?css|eot|[t|o]tf|woff2?)$": "jest-transform-stub"
    },
    "testRegex": "\\.test\\.[t|j]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
